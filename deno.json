{
  "name": "@mizchi/skillize",
  "version": "0.1.1",
  "exports": {
    ".": "./main.ts",
    "./lib": "./mod.ts"
  },
  "tasks": {
    "start": "deno run --allow-read --allow-write --allow-run --allow-net main.ts",
    "check": "deno check main.ts",
    "test": "deno test --allow-read --allow-write --allow-sys",
    "lint:secrets": "deno run -A npm:@secretlint/quick-start \"**/*\""
  },
  "imports": {
    "@mizchi/readability": "npm:@mizchi/readability@^0.7.7",
    "js-yaml": "npm:js-yaml@^4.1.1"
  },
  "compilerOptions": {
    "strict": true
  },
  "license": "MIT",
  "lint": {
    "rules": {
      "exclude": [
        "no-unversioned-import"
      ]
    }
  }
}
